<h1>Admin::Sessions#new</h1>
<p>Find me in app/views/admin/sessions/new.html.erb</p>

<%= form_with url: admin_login_path, scope: :session do |f| %>
  <%= f.label :email, "メールアドレス" %>
  <%= f.email_field :email %>

  <%= f.label :password, "パスワード" %>
  <%= f.password_field :password %>

  <%= f.submit "ログイン" %>
<% end %>
